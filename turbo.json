{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "docs:build": {
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "pretest": {
      "outputMode": "errors-only"
    },
    "test": {
      "dependsOn": ["^test", "pretest"],
      "outputMode": "new-only"
    }
  }
}
